  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "latest",
    "eslint-config-custom": "*",
    "dotenv": "^16.0.1",
    "utils": "*",
    "pure": "*",
    "types": "*",
    "tsconfig": "*"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.17"
}