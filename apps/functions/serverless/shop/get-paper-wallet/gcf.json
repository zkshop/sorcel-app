{
  "params": {
    "useDefaultConfig": true
  },
  "gcf": {},
  "openapi": {
    "post": {
      "summary": "Get paper wallet",
      "parameters": [
        {
          "in": "body",
          "name": "code",
          "required": true,
          "type": "string",
          "description": "Code for the paper wallet"
        }
      ],
      "responses": {
        "200": {
          "description": "Successful operation",
          "schema": {
            "type": "object",
            "properties": {
              "wallet": {
                "type": "object",
                "description": "The paper wallet"
              }
            }
          }
        },
        "500": {
          "description": "Error when requesting the wallet"
        }
      }
    }
  }
}
