{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "lint"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "$NODE_ENV",
        "$VERCEL_URL",
        "$HASURA_API_URL",
        "$HASURA_API_KEY",
        "$PUBLIC_STRIPE_PUBLISHABLE_KEY",
        "$STRIPE_SECRET_KEY",
        "$POAP_API_KEY",
        "$APP_ID",
        "$PUBLIC_MAGIC_PUBLISHABLE_KEY",
        "$MAGIC_SECRET_KEY",
        "$SUPABASE_API_KEY",
        "$SENDGRID_API_KEY",
        "$EMAIL_ORDER_TARGET",
        "$SENDINBLUE_API_KEY"
      ],
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    },
    "generate:graphql": {
      "cache": false,
      "dependsOn": ["$HASURA_API_URL", "$HASURA_API_KEY"]
    }
  }
}
