{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "globalEnv": [
    "ALCHEMY_SECRET_KEY",
    "APP_ID",
    "EMAIL_ORDER_TARGET",
    "FUNCTIONS_API",
    "HASURA_API_KEY",
    "HASURA_API_URL",
    "MAGIC_SECRET_KEY",
    "NODE_ENV",
    "PAPER_API_KEY",
    "PAPER_CLIENT_ID",
    "PAPER_KEY_MANAGER_TOKEN",
    "POAP_API_KEY",
    "PUBLIC_MAGIC_PUBLISHABLE_KEY",
    "PUBLIC_STRIPE_PUBLISHABLE_KEY",
    "SENDINBLUE_API_KEY",
    "STRIPE_SECRET_KEY",
    "SUPABASE_API_KEY",
    "VERCEL_URL"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "storybook-static/**"]
    },
    "dev": {
      "dependsOn": [],
      "cache": false
    },
    "generate:graphql": {
      "cache": false,
      "env": ["HASURA_API_URL", "HASURA_API_KEY"]
    },
    "lint": {},
    "test": {
      "dependsOn": ["lint", "build"]
    }
  }
}
