{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "globalEnv": [
    "APP_ID",
    "EMAIL_ORDER_TARGET",
    "NODE_ENV",
    "PAPER_CLIENT_ID",
    "PAPER_KEY_MANAGER_TOKEN",
    "PUBLIC_FUNCTIONS_URL",
    "PUBLIC_HASURA_API_URL",
    "PUBLIC_MAGIC_PUBLISHABLE_KEY",
    "PUBLIC_STRIPE_PUBLISHABLE_KEY",
    "SECRET_JWT",
    "SECRET_ALCHEMY",
    "SECRET_HASURA",
    "SECRET_MAGIC",
    "SECRET_JWT",
    "SECRET_PAPER",
    "SECRET_POAP",
    "SECRET_SENDINBLUE",
    "SECRET_STRIPE",
    "SECRET_SUPABASE",
    "SENDINBLUE_API_KEY",
    "STRIPE_SECRET_KEY",
    "SUPABASE_API_KEY",
    "CI"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "storybook-static/**"]
    },
    "dev": {
      "dependsOn": [],
      "cache": false
    },
    "test:e2e": {
      "cache": false,
      "env": ["PUBLIC_HASURA_API_URL", "SECRET_JWT", "APP_ID", "PLAYWRIGHT_TEST_BASE_URL"]
    },
    "generate:graphql": {
      "cache": false,
      "env": ["PUBLIC_HASURA_API_URL", "SECRET_HASURA"]
    },
    "lint": {},
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
