{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "$NODE_ENV",
        "$HOSTNAME",
        "$HASURA_API_URL",
        "$HASURA_API_KEY",
        "$PUBLIC_STRIPE_PUBLISHABLE_KEY",
        "$STRIPE_SECRET_KEY",
        "$POAP_API_KEY"
      ],
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "build-storybook": {
      "outputs": [
        "storybook-static/**"
      ]
    }
  }
}